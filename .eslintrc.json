{
  "env": {
    "es2022": true,
    "node": true
  },
  "extends": [
    "standard",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "prefer-const": "error",
    "no-unused-vars": "warn",
    "no-undef": "error",
    "prettier/prettier": "error",
    "space-before-function-paren": "off",
    "comma-dangle": "off"
  }
}
